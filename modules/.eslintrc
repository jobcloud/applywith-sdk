{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all",
      "bracketSpacing": false,
      "parser": "flow"
    }]
  },
  "env": {
    "browser": true
  },
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
  }
}
